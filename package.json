{
  "name": "windcave-vtex-backend-vtex",
  "version": "0.0.2",
  "description": "windcave-vtex backend vtex",
  "main": "dist/index.js",
  "author": "Conexa",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "start": "tsc --build && pm2 start ecosystem.config.json --no-daemon",
    "build": "tsc --build",
    "build:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development nodemon --experimental-modules --es-module-specifier-resolution=node dist/index.js",
    "dev": "concurrently --kill-others \"yarn build:watch\" \"yarn pre:dev\"",
    "test": "npx tsc && jest -i --colors --verbose --detectOpenHandles --passWithNoTests",
    "test:ts": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles --passWithNoTests -- 'ts$'",
    "test:js": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --colors --verbose --detectOpenHandles --passWithNoTests -- 'js$'",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --watchAll --passWithNoTests",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage",
    "coverage:coveralls": "cross-env NODE_OPTIONS=--experimental-vm-modules jest -i --coverage --coverageReporters=lcov",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "prepare": "husky install",
    "commit": "git add -A && cz",
    "postinstall": "husky install",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "release": "standard-version",
    "release:first": "standard-version --first-release",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "generate:docs": "node ./src/lib/scripts/postmanConverter.js",
    "install-all": "yarn add vtex-package-ts@latest --registry https://npm-registry.conexa.ai"
  },
  "keywords": [],
  "devDependencies": {
    "@faker-js/faker": "^6.2.0",
    "@jest/globals": "^29.3.1",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "2.8.8",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^29.2.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.47",
    "@types/nodemailer": "^6.4.4",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "concurrently": "^7.1.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.4.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.5.1",
    "supertest": "^6.2.3",
    "ts-jest": "^29.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@types/express-serve-static-core": "^4.17.30",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^1.2.2",
    "compression": "^1.7.4",
    "conexa-core-server": "^0.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.1.0",
    "express-rate-limit": "^5.5.1",
    "guatapay-sdk": "^0.0.4",
    "helmet": "^4.6.0",
    "http-status": "^1.5.0",
    "husky": "^8.0.2",
    "joi": "^17.5.0",
    "moment": "^2.29.2",
    "mongoose": "^6.1.1",
    "morgan": "^1.10.0",
    "nock": "^13.2.9",
    "pm2": "^5.1.2",
    "postman-to-openapi": "^2.9.0",
    "standard-version": "^9.5.0",
    "swagger-ui-express": "^4.5.0",
    "validator": "^13.7.0",
    "vtex-package-ts": "^0.0.17",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  }
}
